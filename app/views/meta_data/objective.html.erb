<%= content_tag :table, :class => "metadata-table" do
		@meta_data.collect do |meta_datum|
			content_tag :tr do
				a = content_tag :td do
                  content_tag :label do
					meta_datum.first.to_s
                  end
				end
				a += content_tag :td do
					meta_datum.last.to_s
				end
			end
		end.join.html_safe
end %>
